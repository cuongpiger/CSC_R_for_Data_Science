   "source": [
    "number = as.numeric(readline(prompt=\"Enter a number:\"))\n",
    "\n",
    "if (number %% 2) {\n",
    "    sprintf(\"%f is an odd number.\", number)\n",
    "} else {\n",
    "    sprintf(\"%f is an even number.\", number)\n",
    "}"
   ]
